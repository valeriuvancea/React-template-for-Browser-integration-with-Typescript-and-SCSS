{
  "name": "react-webpack-typescript-babel-scss",
  "version": "1.0.0",
  "description": "A sample setup using React with SCSS, Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "webpack --progress --colors --mode=production",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --open --mode=development"
  },
  "author": {
    "name": "Valeriu-Nicolas Vancea",
    "email": "valeriuvancea@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "fork-ts-checker-webpack-plugin": "5.0.5",
    "html-webpack-plugin": "4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^9.0.2",
    "typescript": "3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
